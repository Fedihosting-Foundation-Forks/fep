{
  "@context": {
    "as": "https://www.w3.org/ns/activitystreams#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "fd": "https://www.w3id.org/fep/5a4f#",
    "Topic": "fd:Topic",
    "Vote": "fd:Vote",
    "Voter": "fd:Voter",
    "against": {
      "type": "@id",
      "@id": "fd:against",
      "@container": "@list"
    },
    "againstCount": {
      "@type": "xsd:nonNegativeInteger",
      "@id": "fd:againstCount"
    },
    "algorithm": {
      "@type": "@id",
      "@id": "fd:algorithm"
    },
    "allowsNeutralResponses": {
      "@type": "xsd:boolean",
      "@id": "fd:allowsNeutralResponses"
    },
    "ended": {
      "@type": "xsd:dateTime",
      "@id": "fd:ended"
    },
    "inFavour": {
      "type": "@id",
      "@id": "fd:inFavour",
      "@container": "@list"
    },
    "inFavourCount": {
      "@type": "xsd:nonNegativeInteger",
      "@id": "fd:inFavourCount"
    },
    "neutral": {
      "type": "@id",
      "@id": "fd:neutral",
      "@container": "@list"
    },
    "neutralCount": {
      "@type": "xsd:nonNegativeInteger",
      "@id": "fd:neutralCount"
    },
    "result": {
      "@type": "xsd:boolean",
      "@id": "fd:result"
    },
    "topic": {
      "@type": "@id",
      "@id": "fd:topic"
    },
    "voter": {
      "type": "@id",
      "@id": "fd:vector"
    },
    "voterCount": {
      "@type": "xsd:nonNegativeInteger",
      "@id": "fd:voterCount"
    },
    "voters": {
      "type": "@id",
      "@id": "fd:voters",
      "@container": "@list"
    },
    "wasVetoed": {
      "@type": "xsd:boolean",
      "@id": "fd:wasVetoed"
    }
  }
}